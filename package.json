{
  "name": "jinnmail-api",
  "version": "0.0.0",
  "description": "jinnmail api",
  "main": "server.js",
  "private": true,
  "scripts": {
    "clean": "rm -rf build && mkdir build && mkdir ./build/logs",
    "debug": "babel-watch ./src/server.js --presets es2015,stage-2",
    "build-server": "babel -d ./build ./src -s --presets es2015,stage-2",
    "build": "npm run clean && npm run build-server && npm run lint",
    "start": "node ./build/server.js",
    "lint": "touch ./node_modules/.eslintignore && eslint ./src --fix"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "async": "^2.6.1",
    "bcrypt-nodejs": "0.0.3",
    "bunyan": "^1.8.12",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^6.1.0",
    "express": "~4.16.0",
    "express-validator": "^5.3.0",
    "formidable": "^1.2.1",
    "jsonwebtoken": "^8.2.0",
    "mongoose": "^5.0.12",
    "morgan": "~1.9.0",
    "nodemailer": "^4.6.5",
    "rand-token": "^0.4.0",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "sendgrid": "^5.2.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-watch": "^2.0.7",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0"
  }
}
